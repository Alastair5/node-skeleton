<headers>
  <nav>
    <div class="navTop">
      <div class="navTitle">
        <i class="logo fa-solid fa-car-rear"></i>
        <i class="logoBorder fa-solid fa-car-rear"></i>
        <span>CAR-D-B</span>
      </div>
      <div class="navSearch">
        <input placeholder="   Search..." />
        <button>Search</button>
        <div class="filter">
          Filter
          <i class="fa-solid fa-filter"></i>
        </div>
      </div>
      <div class="navRight">
        <span class="username">Username</span>
        <button>Logout</button>
      </div>
    </div>
    <div class="navBottom">
      <div class="tabs">messages</div>
      <div class="tabs">favorites</div>
    </div>
    <div class="newPost">
      <i class="fa-solid fa-plus"></i>
    </div>
  </nav>
  <div class="filterOptions">
    <div>
      <label for="carMake">Make</label>
      <select id="carMake">
        <option value="">Select a Make</option>
        <option value="Honda">Honda</option>
        <option value="Toyota">Toyota</option>
      </select>
    </div>
    <div>
      <label for="carMake">Model</label>
      <select id="carModel">
        <option value="" data-tag="">Select a Model</option>
        <option value="civic" data-tag="Honda">Civic</option>
        <option value="crv" data-tag="Honda">CRV</option>
        <option value="rav4" data-tag="Toyota">Rav4</option>
      </select>
    </div>
    <div>
      <label for="transmission">Transmission</label>
      <select id="carTransmission">
        <option value="">Select an Option</option>
        <option value="Automatic">Automatic</option>
        <option value="Manual">Manual</option>
      </select>
    </div>
    <div>
      <label for="year">Year</label>
      <input type="number" name="carYear" placeholder="Year" id="carYear" />
    </div>
    <div>
      <label for="minPrice">Min Price</label>
      <input
        type="number"
        name="minPrice"
        placeholder="Min Price"
        id="minPrice"
      />
      <label for="maxPrice">Max Price</label>
      <input
        type="number"
        name="maxPrice"
        placeholder="Max Price"
        id="maxPrice"
      />
    </div>
  </div>
  <section class="newForm">
    <header>
      <h2>Create New Listing</h2>
    </header>
    <div>
      <label for="thumbnail">Thumbnail Image</label>
      <input placeholder="Thumbnail Image" type="text" name="thumbnail_url" id="carthumbnail">
    </div>
    <section class="makeModel">
      <div>
        <label for="carMake">Make</label>
        <select id="make">
          <option value="">Select a Make</option>
        </select>
      </div>
      <div>
        <label for="carMake">Model</label>
        <select id="model">
          <option value="" data-tag="">Select a Model</option>
        </select>
      </div>
      <div>
        <label for="transmission">Transmission</label>
        <select id="transmission">
          <option value="">Select an Option</option>
          <option value="Automatic">Automatic</option>
          <option value="Manual">Manual</option>
        </select>
      </div>
    </section>
    <section>
      <div>
        <label for="year">Year</label>
        <input type="number" name="year" placeholder="Year" id="year" />
      </div>
      <div>
        <label for="odometer">Mileage</label>
        <input type="number" name="odometer" placeholder="Mileage" id="odometer" />
      </div>
    </section>
    <div>
      <label for="price">Price</label>
      <input type="number" name="price" placeholder="Price" id="price" />
    </div>
    <div>
      <label for="color">Color</label>
      <input type="text" name="color" placeholder="Color" id="color" />
    </div>
    <div>
      <textarea placeholder="Description" name="description" id="description" cols="60" rows="5"></textarea>
    </div>
    <div>
      <button>Create</button>
      <a id="property-form__cancel" href="#">Cancel</a>
  </div>
  </section>
</headers>
